source=("manjarofetch")
sha512sums=("SKIP")
package() {
  mkdir -p "${pkgdir}/usr/bin"
  cp "${srcdir}/manjarofetch" "${pkgdir}/usr/bin/manjarofetch"
  chmod +x "${pkgdir}/usr/bin/manjarofetch"
} 

 
